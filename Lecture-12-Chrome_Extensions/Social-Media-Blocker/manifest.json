{
    "name": "FB  Blocker",
    "version": "1.0",
    "description": "It blocks the websites",
    "manifest_version": 2,
    "browser_action": {
        "default_popup": "hello.html"
    },
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": true
    },
    "permissions": [
        "tabs"
    ]
}